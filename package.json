{
  "name": "pizza-restuarant",
  "version": "1.0.0",
  "description": "This is a basic app generator. I find it annoying to keep creating a new app with basically the same folders and files needed for own projects.",
  "main": "app.js",
  "scripts": {
    "test": "npm test",
    "db:create": "createdb pizza_restuarant",
    "db:drop": "dropdb pizza_restuarant",
    "db:schema": "psql -d pizza_restuarant < ./models/schema.sql",
    "db:seed": "psql -d pizza_restuarant < ./models/seed.sql",
    "start": "nodemon app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronev/pizza-restuarant.git"
  },
