  "bugs": {
    "url": "https://github.com/aaronev/pizza-restuarant/issues"
  },
  "homepage": "https://github.com/aaronev/pizza-restuarant#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg-promise": "^6.2.3"
  },
